{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "headphones.png",
      "default_popup": "popup.html",
      "default_title": "Sign in"
   },
   "content_scripts": [ {
      "js": [ "WILT.js" ],
      "matches": [ "*://*.pandora.com/*",
                  "*://*.bandcamp.com/*",
                  "*://*.play.google.com/*",
                  "*://*.youtube.com/*",
                  "*://*.spotify.com/*",
                  "*://*.mutantradio.org/*" ],
      "run_at": "document_end"
   } ],
   "description": "Scrobbles music to What I Listen To",
   "key": "",
   "manifest_version": 2,
   "name": "Wilt Scrobbler",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3"
}
